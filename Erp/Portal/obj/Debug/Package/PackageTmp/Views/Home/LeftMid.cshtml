@{
    Layout = null;
}

<!DOCTYPE html>

<html>
<head>
    <script>
        function ChangeState() {
            if (parent.document.getElementById("fs").cols == "0,7,*")
            {
                parent.document.getElementById("fs").cols = "180,7,*";

            } else {
                parent.document.getElementById("fs").cols = "0,7,*";
            }
        }
    </script>
</head>
<body style="background:#00ffff;width:100%;height:9999px;margin:0" onclick="ChangeState();">
</body>
</html>
