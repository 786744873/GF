    <script language="JavaScript" src="/theme/js/jquery.js"></script>
    <script type="text/javascript">        
        $(function () {
            //导航切换
            $(".menuson li").click(function () {
                $(".menuson li.active").removeClass("active")
                $(this).addClass("active");
                window.parent.parent.leftmenuname = $(this).find("a").text();
            });
            $('.title').click(function () {               
                var $ul = $(this).next('ul');
                $('dd').find('ul').slideUp();
                if ($ul.is(':visible')) {
                    $(this).next('ul').slideUp();
                } else {
                    $(this).next('ul').slideDown();
                }
            });

            //重新处理"点击左侧导航菜单"，目的加入"加载中"控件,hety,2015-09-28
            var $leftmenu = $('.leftmenu');
            $("a", $leftmenu).each(function () {
                $(this).click(function (event) {
                    event.preventDefault();
                    var $this = $(this);
                    top.showBasicLoading();//显示"加载中"控件
                    var switchUrl = $this.attr('href');
                    window.parent.frames["rightFrame"].location.href = switchUrl;
                });
            });

        });
        function ChangeState() {
            if (parent.document.getElementById("fs").cols == "23,*") {
                parent.document.getElementById("fs").cols = "187,*";
                document.getElementById("hidDiv").style.display = "none";

            } else {
                parent.document.getElementById("fs").cols = "23,*";
                document.getElementById("hidDiv").style.display = "block";
            }
        }
    </script>
</head>
