@using CommonService.Model;
@model IEnumerable<CommonService.Model.C_Contacts_experience>
@{
    Layout = "~/Views/Shared/_ListSearchTemplate.cshtml";//普通列表页面固定模板
}
@{
    ViewBag.Title = "联系人工作经历列表";
}
@{
    C_Contacts_experience exConditon = ViewBag.ExpConditon;//联系人工作经历条件模型
}

<table class="tablelist">
    <thead>
        <tr>
            <th>担任职务</th>
            <th>任职时间</th>
            <th>工作内容</th>
        </tr>
    </thead>
    <tbody>
        @foreach (var item in Model)
        {
            <tr target="sid_Iterm" rel="@item.C_Contacts_experience_id">
                <td>@item.C_Contacts_experience_post</td>
                <td>@(item.C_Contacts_experience_date == null ? "" : item.C_Contacts_experience_date.Value.ToString("yyyy-MM-dd"))</td>
                <td>@item.C_Contacts_experience_content</td>
            </tr>
        }
    </tbody>
</table>    