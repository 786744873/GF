@using CommonService.Model;
@model IEnumerable<CommonService.Model.C_CRival_legal_management_level>
@{
    Layout = "~/Views/Shared/_ListSearchWithDialogTemplate.cshtml";//带表单查询的列表页面固定模板
}
@{
    ViewBag.Title = "法律管理水平信息";
}

@{
    C_CRival_legal_management_level crConditon = ViewBag.CrConditon;//法律管理水平条件模型
}
@{
    List<CommonService.Model.C_Parameters> Tf = ViewBag.Tf;
    List<CommonService.Model.C_Parameters> Ms = ViewBag.Ms;
    List<CommonService.Model.C_Parameters> Habit = ViewBag.Habit;
    List<CommonService.Model.C_Parameters> Am = ViewBag.Am;
    List<CommonService.Model.C_Parameters> Sduty = ViewBag.Sduty;
}
<div class="tools">
    <ul class="toolbar">
        <li><a target="popdialog" title="法律管理水平信息" dialogwidth="600" dialogheight="450" href="/basedata/CRival_legal_management_level/create?C_CRival_code=@crConditon.C_CRival_code"><span><img src="/theme/images/new_add.png" /></span>添加</a></li>
        <li><a target="popdialog" title="法律管理水平信息" dialogwidth="600" dialogheight="450" edit="edit" href="/basedata/CRival_legal_management_level/edit?CRival_legal_management_level_id={sid_Iterm}"><span><img src="/theme/images/new_edit.png" /></span>修改</a></li>
        <li><a target="ajaxtodo" title="确认要删除选中数据吗?" singleoperate="singleoperate" href="/basedata/CRival_legal_management_level/delete?CRival_legal_management_level_id={sid_Iterm}"><span><img src="/theme/images/new_delete.png" /></span>删除</a></li>
    </ul>
</div>

<table class="tablelist">
    <thead>
        <tr>
            <th>格式文本</th>
            <th>协商文本</th>
            <th>管理风格</th>
            <th>使用习惯</th>
            <th>印章责任规避</th>
            <th>诉讼管理</th>
        </tr>
    </thead>
    <tbody>
        @foreach (var item in Model)
        {
            <tr target="sid_Iterm" rel="@item.C_CRival_legal_management_level_id">
                @{
                    var tf = Tf.Where(t => t.C_Parameters_id == item.C_CRival_legal_management_level_tf).FirstOrDefault();
                }
                @if(tf!=null)
                {
                    <td>@tf.C_Parameters_name</td>
                }else
                {
                    <td></td>
                }
                <td>@item.C_CRival_legal_management_level_nt</td>
                @{
                    var ms = Ms.Where(t => t.C_Parameters_id == item.C_CRival_legal_management_level_ms).FirstOrDefault();
                }
                @if (ms != null)
                {
                    <td>@ms.C_Parameters_name</td>
                }
                else
                {
                    <td></td>
                }
                @{
                    var habit = Habit.Where(t => t.C_Parameters_id == item.C_CRival_legal_management_level_habit).FirstOrDefault();
                }
                @if (habit != null)
                {
                    <td>@habit.C_Parameters_name</td>
                }
                else
                {
                    <td></td>
                }
                @{
                    var sduty = Sduty.Where(t => t.C_Parameters_id == item.C_CRival_legal_management_level_sduty).FirstOrDefault();
                }
                @if (sduty != null)
                {
                    <td>@sduty.C_Parameters_name</td>
                }
                else
                {
                    <td></td>
                }
                @{
                    var am = Am.Where(t => t.C_Parameters_id == item.C_CRival_legal_management_level_am).FirstOrDefault();
                }
                @if (am != null)
                {
                    <td>@am.C_Parameters_name</td>
                }
                else
                {
                    <td></td>
                }
            </tr>
        }
    </tbody>
</table>