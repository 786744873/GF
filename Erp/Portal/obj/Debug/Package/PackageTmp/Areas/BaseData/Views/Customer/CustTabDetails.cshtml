@{
    Layout = "~/Views/Shared/_TabIframeTemplate.cshtml";//带Tab Iframe 页签切换的页面固定模板
}
@{
    ViewBag.Title = "客户详细";
}
<div class="itab">
    <ul>
        <li><a href="/basedata/customer/details?customerCode=@ViewBag.CustomerCode" tabpanelwidth="100%" tabpanelheight="450" class="selected">基本信息</a></li>
        <li><a href="/basedata/company/viewlist?customerCode=@ViewBag.CustomerCode" tabpanelwidth="100%" tabpanelheight="450">公司信息</a></li>
        <li><a href="/basedata/contacts/viewlist?relationCode=@ViewBag.CustomerCode&contactType=2" tabpanelwidth="100%" tabpanelheight="450">联系人信息</a></li>
        <li><a href="/basedata/bank/viewlist?customerCode=@ViewBag.CustomerCode" tabpanelwidth="100%" tabpanelheight="450">银行信息</a></li>
        <li><a href="/basedata/address/viewlist?customerCode=@ViewBag.CustomerCode" tabpanelwidth="100%" tabpanelheight="450">客户地址信息</a></li>
       
    </ul>
</div>
