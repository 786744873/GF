@using CommonService.Model;
@model IEnumerable<CommonService.Model.C_PRival_education>
@{
    Layout = "~/Views/Shared/_ListSearchWithDialogTemplate.cshtml";//带表单查询的列表页面固定模板
}
@{
    ViewBag.Title = "教育背景列表";
}

@{
    C_PRival_education eduConditon = ViewBag.EduConditon;//个人教育背景条件模型
}
<div class="tools">
    <ul class="toolbar">
        <li><a target="popdialog" title="教育背景" dialogwidth="600" dialogheight="350" href="/basedata/PRival_education/create?prival_code=@eduConditon.C_PRival_code"><span><img src="/theme/images/new_add.png" /></span>添加</a></li>
        <li><a target="popdialog" title="教育背景" dialogwidth="600" dialogheight="350" edit="edit" href="/basedata/PRival_education/edit?prival_education_id={sid_Iterm}"><span><img src="/theme/images/new_edit.png" /></span>修改</a></li>
        <li><a target="ajaxtodo" title="确认要删除选中数据吗?" singleoperate="singleoperate" href="/basedata/PRival_education/delete?prival_education_id={sid_Iterm}"><span><img src="/theme/images/new_delete.png" /></span>删除</a></li>

    </ul>
</div>

<table class="tablelist">
    <thead>
        <tr>
            <th>教育程度</th>
            <th>毕业时间</th>
            <th>毕业院校</th>
            <th>毕业专业</th>
        </tr>
    </thead>
    <tbody>
        @foreach (var item in Model)
        {
            <tr target="sid_Iterm" rel="@item.C_PRival_education_id">
                <td>@item.C_PRival_education_degree</td>
                <td>@(item.C_PRival_education_graduation_date==null ? "" : item.C_PRival_education_graduation_date.Value.ToString("yyyy-MM-dd"))</td>
                <td>@item.C_PRival_education_school</td>
                <td>@item.C_PRival_education_science</td>
            </tr>
        }
    </tbody>
</table>    