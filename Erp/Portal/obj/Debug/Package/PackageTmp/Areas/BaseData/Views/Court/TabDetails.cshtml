@using CommonService.Common;
@{
    Layout = "~/Views/Shared/_TabIframeTemplate.cshtml";//带Tab Iframe 页签切换的页面固定模板
}
@{
    ViewBag.Title = "法院详细";
}
<div class="itab">
    <ul>
        <li><a href="/basedata/Court/Details?courtCode=@ViewBag.CourtCode" tabpanelwidth="100%" tabpanelheight="500" class="selected">法院信息</a></li>
        <li><a href="/basedata/Court/PersonComponentDetails?courtCode=@ViewBag.CourtCode" tabpanelwidth="100%" tabpanelheight="800">人员组成</a></li>
        <li><a href="/attachmentupload/attachmentlist?relationCode=@ViewBag.CourtCode&targetDirectory=@BaseEnumHelper.GetUploadAttachmentFolder(FileBelongTypeEnum.法院)&fileBelongType=@Convert.ToInt32(FileBelongTypeEnum.法院)" tabpanelwidth="100%" tabpanelheight="600">附件信息</a></li>
        
    </ul>
</div>
