@using CommonService.Model;

@model CommonService.Model.C_Involved_project
@{
    Layout = "~/Views/Shared/_ViewPanelTemplate.cshtml";//普通Panel页面固定模板
}
@{
    ViewBag.Title = "涉案项目信息";
}

<div class="formbody">
    <ul class="forminfo">
        <li><label>工程名称：</label><span>@Model.C_Involved_project_name</span></li>
        <li><label>工程地点：</label><span>@Model.C_Involved_project_address</span></li>
        <li><label>工程类别：</label><span>@Model.C_Involved_project_type</span></li>
        <li><label>工程建设规模(㎡)：</label><span>@Model.C_Involved_project_scale</span></li>
        <li><label>工程建设投资：</label><span>@Model.C_Involved_project_Investment</span></li>
    </ul>
    </div>
