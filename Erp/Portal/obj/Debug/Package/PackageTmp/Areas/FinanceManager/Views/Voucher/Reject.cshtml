@model CommonService.Model.FinanceManager.C_Voucher
@{
    Layout = "~/Views/Shared/_EditDialogFormTemplate.cshtml";//带表单提交的编辑页面固定模板
}
@{
    ViewBag.Title = "驳回信息";
}
@section HeadScript{
    <script type="text/javascript">
        document.onkeyup = function (e) {      //onkeyup是javascript的一个事件、当按下某个键弹起 var _key; //的时触发
            if (e == null) { // ie
                _key = event.keyCode;
            } else { // firefox              //获取你按下键的keyCode
                _key = e.which;          //每个键的keyCode是不一样的
            }
            if (_key == 13) {   //判断keyCode是否是13，也就是回车键(回车的keyCode是13)
                onlySaveForm(document.getElementById('btnSave'));
            }
        }
    </script>
}
<div class="formbody">
    <div class="formtitle"><span>基本信息</span></div>
    <ul class="forminfo">
        @using (Html.BeginForm("SaveReject", "Voucher", FormMethod.Post, new { id = "formOrganization", onsubmit = "return validateCallback(this,dialogAjaxDone);" }))
        {
             @Html.Hidden("C_Voucher_id", Model.C_Voucher_id)
             @Html.Hidden("C_Voucher_code", Model.C_Voucher_code)
             @Html.Hidden("C_Voucher_number", Model.C_Voucher_number)
             @Html.Hidden("C_Voucher_state", Model.C_Voucher_state)
             @Html.Hidden("C_Voucher_applicationTime", Model.C_Voucher_applicationTime)
             @Html.Hidden("C_Voucher_applicationPerson", Model.C_Voucher_applicationPerson)            
             @Html.Hidden("C_Voucher_documentType", Model.C_Voucher_documentType)
             @Html.Hidden("C_Voucher_department", Model.C_Voucher_department)
             @Html.Hidden("C_Voucher_amounts", Model.C_Voucher_amounts)
             @Html.Hidden("C_Voucher_superiorAuditPerson", Model.C_Voucher_superiorAuditPerson)
             @Html.Hidden("C_Voucher_financeConfirmPerson", Model.C_Voucher_financeConfirmPerson)
             @Html.Hidden("C_Voucher_confirmTime", Model.C_Voucher_confirmTime)
             @Html.Hidden("C_Voucher_auditStatus", Model.C_Voucher_auditStatus)
             @Html.Hidden("C_Voucher_paymentMethod", Model.C_Voucher_paymentMethod)
             @Html.Hidden("C_Voucher_isDelete", Model.C_Voucher_isDelete)
             @Html.Hidden("C_Voucher_creator", Model.C_Voucher_creator)
             @Html.Hidden("C_Voucher_createTime", Model.C_Voucher_createTime)
             <li><label>驳回理由</label>@Html.TextArea("C_Voucher_dismissedReasons", Model.C_Voucher_dismissedReasons, new { @class = "textinput", maxlength = "500" })<i></i></li>
             <li class="buttonWrap clear"><label>&nbsp;</label><input name="" type="button" id="btnSave" onclick="onlySaveForm(this)" class="btn btnmr fl" value="确认保存" /><i></i></li>
        }
    </ul>
</div>