@using CommonService.Common;
@{
    Layout = "~/Views/Shared/_TabIframeTemplate.cshtml";//带Tab Iframe 页签切换的页面固定模板
}
@{
    ViewBag.Title = "案件详细";
}
<div class="itab">
    <ul>
        <li><a href="/casemanager/case/details?caseCode=@ViewBag.caseCode" tabpanelwidth="100%" tabpanelheight="800" class="selected">案件内容</a></li>
        <li><a href="/casemanager/clientagreement/tabdetailed?caseCode=@ViewBag.caseCode&type=1" tabpanelwidth="100%" tabpanelheight="800">客户约定</a></li>
        <li><a href="/AttachmentCaseUpload/AttachmentCaseList?relationCode=@ViewBag.caseCode&targetDirectory=@BaseEnumHelper.GetUploadAttachmentFolder(FileBelongTypeEnum.案件)&fileBelongType=@Convert.ToInt32(FileBelongTypeEnum.案件)" tabpanelwidth="100%" tabpanelheight="800">收案材料清单</a></li>
        <li><a href="/casemanager/rdetail/viewlist?caseCode=@ViewBag.caseCode&type=1" tabpanelwidth="100%" tabpanelheight="800">收款明细</a></li>
    </ul>
</div>
