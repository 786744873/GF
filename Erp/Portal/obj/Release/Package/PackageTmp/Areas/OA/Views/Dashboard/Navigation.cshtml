@{
    Layout = null;
}
<script type="text/javascript">
    var id = '@ViewBag.Id';
    var nav = '@ViewBag.Nav';
   
    if (nav.indexOf('submitform_') > -1) {
        //加载提交表单页面
        $('#a_ajaxify').attr('href', '/oa/form/createandsave?id=' + id + "&nav=" + nav);
        $('#a_ajaxify').click();//触发关联异步Loading页面
    }
</script>