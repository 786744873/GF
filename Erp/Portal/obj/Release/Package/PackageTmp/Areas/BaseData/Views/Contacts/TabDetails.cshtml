@using CommonService.Common;
@{
    Layout = "~/Views/Shared/_TabIframeTemplate.cshtml";//带Tab Iframe 页签切换的页面固定模板
}
@{
    ViewBag.Title = "联系人详细";
}
<div class="itab">
    <ul>
        <li><a href="/basedata/Contacts/Details?C_Contacts_code=@ViewBag.C_Contacts_code" tabpanelwidth="100%" tabpanelheight="500" class="selected">基本信息</a></li>
        <li><a href="/basedata/Contacts_education/viewlist?C_Contacts_code=@ViewBag.C_Contacts_code" tabpanelwidth="100%" tabpanelheight="500">教育背景</a></li>
        <li><a href="/basedata/Contacts_experience/viewlist?C_Contacts_code=@ViewBag.C_Contacts_code" tabpanelwidth="100%" tabpanelheight="500">工作经历</a></li>
        <li><a href="/basedata/Property_trail/PropertyTabDetails?crival_code=@ViewBag.C_Contacts_code&property_trail_type=87" tabpanelwidth="100%" tabpanelheight="500">财产状况</a></li>
        @if(ViewBag.type==1)
        {
            <li><a href="/attachmentupload/attachmentlist?relationCode=@ViewBag.C_Contacts_code&targetDirectory=@BaseEnumHelper.GetUploadAttachmentFolder(FileBelongTypeEnum.法官)&fileBelongType=@Convert.ToInt32(FileBelongTypeEnum.法官)" tabpanelwidth="100%" tabpanelheight="600">附件信息</a></li>
        }else
        {
            <li><a href="/attachmentupload/attachmentlist?relationCode=@ViewBag.C_Contacts_code&targetDirectory=@BaseEnumHelper.GetUploadAttachmentFolder(FileBelongTypeEnum.联系人)&fileBelongType=@Convert.ToInt32(FileBelongTypeEnum.联系人)" tabpanelwidth="100%" tabpanelheight="600">附件信息</a></li>
        }
    </ul>
</div>
