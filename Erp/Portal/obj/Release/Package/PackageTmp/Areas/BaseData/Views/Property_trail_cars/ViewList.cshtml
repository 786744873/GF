@using CommonService.Model;
@model IEnumerable<CommonService.Model.C_Property_trail_cars>
@{
    Layout = "~/Views/Shared/_ListSearchTemplate.cshtml";//普通列表页面固定模板
}
@{
    ViewBag.Title = "财产线索车辆列表";
}
@{
    C_Property_trail_cars carConditon = ViewBag.CarConditon;//财产线索车辆条件模型
}

<table class="tablelist">
    <thead>
        <tr>
            <th>车型</th>
            <th>车辆登记号</th>
            <th>车辆购买金额</th>
            <th>车辆购买时间</th>
            <th>车牌号</th>
        </tr>
    </thead>
    <tbody>
        @foreach (var item in Model)
        {
            <tr target="sid_Iterm" rel="@item.C_Property_trail_cars_id">
                <td>@item.C_Property_trail_cars_models</td>
                <td>@item.C_Property_trail_cars_reCode</td>
                <td>@item.C_Property_trail_cars_price</td>
                <td>@(item.C_Property_trail_cars_buyDate == null ? "" : item.C_Property_trail_cars_buyDate.Value.ToString("yyyy-MM-dd"))</td>
                <td>@item.C_Property_trail_cars_license</td>
            </tr>
        }
    </tbody>
</table>    