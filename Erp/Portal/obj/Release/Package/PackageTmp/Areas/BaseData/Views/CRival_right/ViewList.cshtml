@using CommonService.Model;
@model IEnumerable<CommonService.Model.C_CRival_right>
@{
    Layout = "~/Views/Shared/_ListSearchTemplate.cshtml";//普通列表页面固定模板
}
@{
    ViewBag.Title = "公司权力关联列表";
}
@{
    C_CRival_right crConditon = ViewBag.CrRightConditon;//对手公司权力关联条件模型
}

<table class="tablelist">
    <thead>
        <tr>
            <th>机构</th>
            <th>人物</th>
            <th>事务</th>
            <th>政策</th>
        </tr>
    </thead>
    <tbody>
        @foreach (var item in Model)
        {
            <tr target="sid_Iterm" rel="@item.C_CRival_code">
                <td>@item.C_CRival_right_agency</td>
                <td>@item.C_CRival_right_person</td>
                <td>@item.C_CRival_right_affairs</td>
                <td>@item.C_CRival_right_Policy</td>
            </tr>
        }
    </tbody>
</table>    