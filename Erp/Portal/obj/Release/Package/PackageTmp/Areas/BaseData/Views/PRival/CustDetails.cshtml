@using CommonService.Common;
@{
    Layout = "~/Views/Shared/_TabIframeTemplate.cshtml";//带Tab Iframe 页签切换的页面固定模板
}
@{
    ViewBag.Title = "个人详细";
}
<div class="itab">
    <ul>
        <li><a href="/basedata/PRival/Details?prival_code=@ViewBag.PRival_code" tabpanelwidth="100%" tabpanelheight="500" class="selected">对手信息</a></li>
        <li><a href="/basedata/PRival_education/viewlist?prival_code=@Model.C_PRival_code" tabpanelwidth="100%" tabpanelheight="500">教育背景</a></li>
        <li><a href="/basedata/PRival_experience/viewlist?prival_code=@Model.C_PRival_code" tabpanelwidth="100%" tabpanelheight="500">工作经历</a></li>
