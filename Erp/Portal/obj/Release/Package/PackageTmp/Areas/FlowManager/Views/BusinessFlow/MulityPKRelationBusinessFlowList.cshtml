@using CommonService.Common;
@{
    Layout = "../Shared/_NoPageBusinessFlowListTemplate.cshtml";//无分页业务流程列表页面固定模板
}
@{
    ViewBag.Title = "案件或商机关联业务流程递归列表";
}
<table class="tablelist">
    <thead>
        <tr>
            @{
                if (Convert.ToInt32(ViewBag.FkType)==Convert.ToInt32(FlowTypeEnum.案件))
                {
                    <th>&nbsp;</th>
                }
            }
            <th>序号</th>
            <th>任务名称</th>
            <th>任务类型</th>
            <th>状态</th>         
        </tr>
    </thead>
    <tbody>
        @Html.Raw(ViewBag.MulityBusinessFlowListHtml)
    </tbody>
</table>
