@{
    Layout = "~/Views/Shared/_MulityListIframeWithCallbackTemplate.cshtml";//多个列表Iframe Callback 页面固定模板
}
@{
    ViewBag.Title = "流程->表单->时间";//单选带回
}
<div class="iframe_callback_list_tool">
    <div class="tools">
        <ul class="toolbar">
            <li><a target="ajaxtodo" notice="请选时间信息！" singleoperate="singleoperate" frametype="iframe" frametarget="iframe_date" frameuitype="list" href="javascript:window.parent.bringBack({callbackjson})"><span></span>确定</a></li>
        </ul>
    </div>
</div>
<table class="table_tree_mulitylist">
    <tr>
        <td class="rightborder" width="33%" height="460">
            <table class="table_noborder" height="100%">
                @*<tr class="bottomborder">
                    <td height="44" align="center"><b class="fontsize14">组织机构</b></td>
                </tr>*@
                <tr>
                    <td height="100%"><iframe name="iframe_tree" width="100%" height="100%" frameborder="0" src="/flowmanager/flow/tree?sourceType=3&type=0" scrolling="auto"></iframe></td>
                </tr>
            </table>
        </td>
        <td class="rightborder" width="33%" height="460">
            <table class="table_noborder" height="100%">
                <tr class="bottomborder">
                    <td height="53" align="center"><b class="fontsize14">表单</b></td>
                </tr>
                <tr>
                    <td height="100%"><iframe name="iframe_forms" width="100%" height="100%" frameborder="0" baseurl="/flowmanager/flow_form/FlowRelationFormListToEntry?flowCode={flowCode}" src="/flowmanager/flow_form/FlowRelationFormListToEntry?flowCode={flowCode}" scrolling="auto"></iframe></td>
                </tr>
            </table>
        </td>
        <td width="*" height="460">
            <table class="table_noborder" height="100%">
                <tr class="bottomborder">
                    <td height="53" align="center"><b class="fontsize14">时间</b></td>
                </tr>
                <tr>
                    <td height="100%"><iframe name="iframe_date" width="100%" height="100%" frameborder="0" baseurl="/customerform/formdatedeclare/formrelationformdatedeclarelist?flowCode={flowCode}&formCode={formCode}&type=@ViewBag.type" src="/customerform/formdatedeclare/formrelationformdatedeclarelist?flowCode={flowCode}&formCode={formCode}&type=@ViewBag.type" scrolling="auto"></iframe></td>
                </tr>
            </table>
        </td>
    </tr>
</table>