@using CommonService.Model.SysManager;
@model IEnumerable<CommonService.Model.SysManager.C_Roles>
@{
    Layout = "~/Views/Shared/_NoPageListTemplate.cshtml";//无分页列表页面固定模板
}
@{
    ViewBag.Title = "角色列表";
}
@{
    C_Roles roleConditon = ViewBag.RoleConditon;//角色条件模型
}
<table class="tablelist">
    <tbody>
        @foreach (var item in Model)
        {
            <tr target="sid_Iterm" rel="@item.C_Roles_id">
                <td><input type="radio" name="rol" value=""></td>
                <td>@item.C_Roles_name</td>
            </tr>
        }
    </tbody>
</table>    
