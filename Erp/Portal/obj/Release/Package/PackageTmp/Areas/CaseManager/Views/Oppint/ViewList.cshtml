@using CommonService.Model.CaseManager;
@model IEnumerable<CommonService.Model.CaseManager.B_Oppint>
@{
    Layout = "~/Views/Shared/_ListSearchTemplate.cshtml";//普通列表页面固定模板
}
@{
    ViewBag.Title = "担保物约定";
}

@{
    B_Oppint Conditon = ViewBag.Conditon;//条件模型
}
<table class="tablelist">
    <thead>
        <tr>
            <th>担保物</th>
            <th>担保物价值</th>
            <th>客户其他需求</th>
        </tr>
    </thead>
    <tbody>
        @foreach (var item in Model)
        {
            <tr target="sid_Iterm" rel="@item.B_Oppint_id">
                <td>@item.B_Oppint_guaranty_name</td>
                <td>@item.B_Oppint_guarantyvalue</td>
                <td>@item.B_Oppint_coneed</td>
            </tr>
        }
    </tbody>
</table>    