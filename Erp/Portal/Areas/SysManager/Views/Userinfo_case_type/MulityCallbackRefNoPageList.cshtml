@using CommonService.Model;
@model IEnumerable<CommonService.Model.SysManager.C_Userinfo_case_type>
@section HeadScript
{
    <script type="text/javascript">
        $(function () {
            $("tbody tr", $globalTablelist).each(function () {
                var $this = $(this);
                if ($this.hasClass('selected')) {
                    window.commonListData.uniqueId = $this.attr('rel');
                }
            });
        });
    </script>
}

@{
    Layout = "~/Views/Shared/_NoPageListTemplate.cshtml";//无分页列表页面固定模板
}
@{
    ViewBag.Title = "区域列表";//单选带回
}
<table class="tablelist">
    <tbody>
        @foreach (var item in Model)
        {
            <tr target="sid_Iterm" rel="@item.C_Parameters_id" align=center>
                <td>@item.C_Parameters_name</td>
            </tr>
        }
    </tbody>
</table>

