@model IEnumerable<CommonService.Model.SysManager.C_Role_Role_Power>
@{
    Layout = "~/Views/Shared/_NoPageListTemplate.cshtml";//无分页列表页面固定模板
}
@{
    ViewBag.Title = "角色-关联角色权限列表";
}
<table class="tablelist">
    <thead>
        <tr>
            <th>权限名称</th>            
        </tr>
    </thead>
    <tbody>
        @foreach (var item in Model)
        {
            <tr target="sid_Iterm" rel="@item.C_Role_Role_Power_id">
                <td>@item.C_Role_Power_name</td>              
            </tr>
        }
    </tbody>
</table>
