@using CommonService.Model.FlowManager;
@model IEnumerable<CommonService.Model.FlowManager.P_Business_flow_form>

@{
    Layout = "~/Views/Shared/_NoPageListTemplate.cshtml";//无分页列表页面固定模板
}
@{
    ViewBag.Title = "业务流程关联表单列表";
}
@section HeadCss{
    <style type="text/css">
        .noData {
            color: gray;
            text-align: center;
        }
    </style>
}
@section HeadScript{
    <script type="text/javascript">
        $(function () {

        })
    </script>
}
<table class="tablelist">
    <tbody>
        @if (Model.Count() == 0)
        {
        <td class="noData">暂无数据</td>
        }
        else
        {
            foreach (var item in Model)
            {
                <tr target="sid_Iterm" rel="@item.P_Business_flow_form_code" onclick="setuniquerid('@item.P_Business_flow_form_code')">

                    @if (!string.IsNullOrEmpty(item.P_Business_flow_form_person_name))
                    {
                        <td>@item.F_Form_chineseName </td>
                    }
                    else
                    {
                        <td>@item.F_Form_chineseName</td>
                    }
                </tr>
            }
        }
        </tbody>
    </table>
    <script>
        var uniqueId;
        function setuniquerid(val) {
            uniqueId = val;
        }
    </script>