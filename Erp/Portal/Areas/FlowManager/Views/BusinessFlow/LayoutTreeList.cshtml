@{
    Layout = null;
}
<!DOCTYPE HTML PUBLIC "-//W3C//DTD XHTML 1.0 Frameset//EN" "http://www.w3.org/TR/xhtml1/DTD/xhtml1-frameset.dtd">
<html xmlns="http://www.w3.org/1999/xhtml">
<head>
    <meta http-equiv="Content-Type" content="text/html; charset=utf-8" />
    <title>北斗ERP系统</title>
    <link href="/theme/css/style.css" rel="stylesheet" type="text/css" />
    <script type="text/javascript" src="/theme/js/jquery-1.10.2.min.js"></script>
    <script type="text/javascript" src="/theme/js/alertMsg.js"></script>
</head>
@{
   string businessFlowCode = ViewBag.BusinessFlowCode;
   string fkCode = ViewBag.FkCode;
   string operatetype = ViewBag.OperateType;
   int type = ViewBag.Type;
}
<frameset cols="260,*" frameborder="no" border="0" framespacing="0">
    <frame src="/flowmanager/flow/Validate_Tree?sourceType=2&businessFlowCode=@businessFlowCode&fkCode=@fkCode&operatetype=@operatetype&type=@type" name="leftTreeFrame" scrolling="auto" class="frameRightBorder" noresize="noresize" id="leftTreeFrame" title="leftTreeFrame" />
    @if (operatetype == "add")
    {
       <frame usetreelink="usetreelink" src="/flowmanager/businessflow/create?businessFlowCode=@businessFlowCode&fkCode=@fkCode&type=@type" name="rightContentFrame" id="rightContentFrame" title="rightContentFrame" />
    }
    else if (operatetype == "edit")
    {
       <frame usetreelink="usetreelink" src="/flowmanager/businessflow/modify?businessFlowCode=@businessFlowCode&type=@type" name="rightContentFrame" id="rightContentFrame" title="rightContentFrame" />
    }    
</frameset>
<noframes>
    <body>
    </body>
</noframes>
</html>
