@using CommonService.Model;
@model IEnumerable<CommonService.Model.C_PRival_experience>
@{
    Layout = "~/Views/Shared/_ListSearchWithDialogTemplate.cshtml";//带表单查询的列表页面固定模板
}
@{
    ViewBag.Title = "工作经历列表";
}

@{
    C_PRival_experience exConditon = ViewBag.ExConditon;//工作经历条件模型
}
<div class="tools">
    <ul class="toolbar">
        <li><a target="popdialog" title="工作经历" dialogwidth="600" dialogheight="300" href="/basedata/PRival_experience/create?prival_code=@exConditon.C_PRival_code"><span><img src="/theme/images/new_add.png" /></span>添加</a></li>
        <li><a target="popdialog" title="工作经历" dialogwidth="600" dialogheight="300" edit="edit" href="/basedata/PRival_experience/edit?prival_experience_id={sid_Iterm}"><span><img src="/theme/images/new_edit.png" /></span>修改</a></li>
        <li><a target="ajaxtodo" title="确认要删除选中数据吗?" singleoperate="singleoperate" href="/basedata/PRival_experience/delete?prival_experience_id={sid_Iterm}"><span><img src="/theme/images/new_delete.png" /></span>删除</a></li>

    </ul>
</div>

<table class="tablelist">
    <thead>
        <tr>
            <th>担任职务</th>
            <th>任职时间</th>
            <th>工作内容</th>
        </tr>
    </thead>
    <tbody>
        @foreach (var item in Model)
        {
            <tr target="sid_Iterm" rel="@item.C_PRival_experience_id">
                <td>@item.C_PRival_experience_post</td>
                <td>@(item.C_PRival_experience_date==null ? "" : item.C_PRival_experience_date.Value.ToString("yyyy-MM-dd"))</td>
                <td>@item.C_PRival_experience_content</td>
            </tr>
        }
    </tbody>
</table>    