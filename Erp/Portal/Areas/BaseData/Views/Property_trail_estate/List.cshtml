@using CommonService.Model;
@model IEnumerable<CommonService.Model.C_Property_trail_estate>
@{
    Layout = "~/Views/Shared/_ListSearchWithDialogTemplate.cshtml";//带表单查询的列表页面固定模板
}
@{
    ViewBag.Title = "房产信息";
}

@{
    C_Property_trail_estate estateConditon = ViewBag.EstateConditon;//对手财产线索房产条件模型
}
<div class="tools">
    <ul class="toolbar">
        <li><a target="popdialog" title="房产信息" dialogwidth="600" dialogheight="200" href="/basedata/Property_trail_estate/create?property_trail_estate_belongs=@estateConditon.C_Property_trail_estate_belongs&property_trail_estate_type=@estateConditon.C_Property_trail_estate_type"><span><img src="/theme/images/new_add.png" /></span>添加</a></li>
        <li><a target="popdialog" title="房产信息" dialogwidth="600" dialogheight="200" edit="edit" href="/basedata/Property_trail_estate/edit?property_trail_estate_id={sid_Iterm}"><span><img src="/theme/images/new_edit.png" /></span>修改</a></li>
        <li><a target="ajaxtodo" title="确认要删除选中数据吗?" singleoperate="singleoperate" href="/basedata/Property_trail_estate/delete?property_trail_estate_id={sid_Iterm}"><span><img src="/theme/images/new_delete.png" /></span>删除</a></li>

    </ul>
</div>

<table class="tablelist">
    <thead>
        <tr>
            <th>房产证号</th>
        </tr>
    </thead>
    <tbody>
        @foreach (var item in Model)
        {
            <tr target="sid_Iterm" rel="@item.C_Property_trail_estate_id">
                <td>@item.C_Property_trail_estate_license</td>
            </tr>
        }
    </tbody>
</table>