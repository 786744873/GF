@{
    Layout = null;
}
@{
    List<CommonService.Model.C_Parameters> ResourcesType = ViewBag.ResourcesType;//资源类型
}
<div class="filter-v1 margin-top-10">
    <div class="col-md-12">
        <ul class="mix-filter col-md-9" id="auditResourcesType">
            类型：
            <li class="filter active" id="">
                不限
            </li>
            <li class="filter" id="1">
                文档
            </li>
            @foreach (var item in ResourcesType)
            {
                if (item.C_Parameters_id == 825 || item.C_Parameters_id == 827 || item.C_Parameters_id == 829)
                {
                    <li class="filter" id="@item.C_Parameters_id">
                        @item.C_Parameters_name
                    </li>
                }
            }
        </ul>
        <div class="col-md-3">
            <div id="KnowledgeAuditlist_search2" class="row">
                @Html.Hidden("isExecutedSearch", "0")
                @Html.Hidden("auditResourcesTypeId")
                <div class="input-group">
                    <input name="s_title" id="s_title" type="text" style="float:right;margin-left:10px;" class="form-control" placeholder="资源名称">
                    <span class="input-group-btn">
                        <button id="auditBtn" class="btn blue" type="button" onclick="executeSearch('KnowledgeAuditlist', 'KnowledgeAuditlist_search2')"><i class="fa fa-search"></i></button>
                    </span>
                </div>
            </div>
        </div>
    </div>
    <div class="portlet box ">
        <div class="portlet-body">
            <table class="table table-striped table-bordered table-hover margin-top-10" id="KnowledgeAuditlist">
                <thead>
                    <tr class="bg-grey">
                        <th class="text-center">资源Guid</th>
                        <th class="text-center">资源类型</th>
                        <th class="text-center">资源Url</th>
                        <th class="text-center">标题</th>
                        <th class="text-center">类型</th>
                        <th class="text-center">知识分类</th>
                        <th class="text-center">审核状态</th>
                        <th class="text-center">下载</th>
                        <th class="text-center">上传人</th>
                        <th class="text-center">上传时间</th>
                        <th class="text-center" style="width:12%;">操作</th>
                    </tr>
                </thead>
                <tbody>
                    <!--数据行在这里-->
                </tbody>
            </table>
        </div>
    </div>
</div>

